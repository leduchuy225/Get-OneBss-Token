{
  "manifest_version": 3,

  "name": "GET OneBSS-Token",
  "description": "Copy OneBss Token to clipboard",
  "version": "1.0",

  "action": {
    "default_icon": { "16": "icon.png", "24": "icon.png", "32": "icon.png" },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "24": "icon.png",
    "32": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage", "scripting"],
  "host_permissions": ["<all_urls>"]
}
